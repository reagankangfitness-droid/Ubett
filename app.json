{
  "expo": {
    "name": "Ubett",
    "slug": "ubett",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ubett",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FDF6EC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ubett.app",
      "infoPlist": {
        "UIBackgroundModes": ["location"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FDF6EC"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ubett.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#E85D26"
        }
      ],
      [
        "expo-background-fetch",
        {
          "stopOnTerminate": false,
          "startOnBoot": true
        }
      ],
      "expo-task-manager",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Ubett uses your location to detect when you leave home so it can remind you to check your essentials.",
          "locationWhenInUsePermission": "Ubett uses your location to set your home position.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
